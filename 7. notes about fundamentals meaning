package main

import (
	"fmt"
)

var x bool

func main() {
	fmt.Println(x) // zero value Ã© false, 
	x = true
	fmt.Println(x) // valor atribuido
	x = 10 == 100
	y := 10 == 100
	z := 10 > 100
	fmt.Println(x)
	fmt.Println(y)
	fmt.Println(z)
}

// outpu 

false 
true 
true
false
false

// bool as a result of this type of operations relational 

# zero: 
ints: 0 
floats : 0.0 
booleans: false 
string: ""
pointers, functions, interfaces, slices, channels, maps: nil
use := always as possible
using var to package-level scope 

package main 
import(
"fmt"
)

var a int 
var b float64
var c string 
var d bool 

func main(){
fmt.Printf("%v, %T\n", a, a)
fmt.Printf("%v, %T\n", b, b)
fmt.Printf("%v, %T\n", c, c)
fmt.Printf("%v, %T\n", d, d)

}

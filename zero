# zero: 
ints: 0 
floats : 0.0 
booleans: false 
string: ""
pointers, functions, interfaces, slices, channels, maps: nil
use := always as possible
using var to package-level scope 

package main
import( " fmt")
var x int

func main() {
	x = 10
	x := 10 
	fmt.printf("%v,%T", x, x)
	x = 20 
	fmt.Printf("%v, %T", x,x)
}

// decimal, binary, hexadec

package main

import (
	"fmt"
)

func main() {
	x := 100
	fmt.Printf("%d, %#x, %b", x, x, x)
}


output:
100, 0x64, 1100100

output 

x := 200
outuput: 200, c8, 1100100

